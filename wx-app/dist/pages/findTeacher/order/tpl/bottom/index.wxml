<view class="order-bottom order-bottom-android" hidden="{{isHiddenBottom}}">
    <view class="phone" bindtap="bindGSXCallTap">
        <view class="line">
            <image mode="scaleToFill" src="https://imgs.genshuixue.com/0cms/d/file/content/2017/06/592fcb9314845.png"></image>
        </view>
        <view class="line {{!isIOS ? 'bottom-text-android' : 'bottom-text-ios'}}">
            电话咨询
        </view>
    </view>
    <view class="wechat">
        <button 
            open-type="contact"
            session-from="weapp"
            plain="true"
            bindtap="bindWechatTap"
            size="default"
        >
            <view class="line">
                <image mode="scaleToFill" src="https://imgs.genshuixue.com/0cms/d/file/content/2017/06/592fcb935393e.png"></image>
            </view>
            <view class="line {{!isIOS ? 'bottom-text-android' : 'bottom-text-ios'}}">
                微信咨询
            </view>
        </button>
    </view>
    <view class="consult" bindtap="bindOrderTap">
        预约名师试听课
    </view>
</view>