.sign-home-protocol {
    padding: rem(16);
    padding-bottom: rem(70);
    font-size: rem(14);
    color: #6D6D6E;
    letter-spacing: rem(-0.53);
    line-height: rem(22);
    .title {
        font-size: rem(18);
        color: #2D2D2E;
        line-height: rem(60);
        text-align: center;
        font-weight: 500;
    }   
    .t1 {
        font-size: rem(14);
        color: #000;
        letter-spacing: rem(-0.53);
        line-height: rem(32);
        font-weight: 500;
        margin-top: rem(22);
    }
    .t2 {
        .subject,.ratio {
            color: #ff6c00;
            margin-right: rem(5);
        }
        .ratio {
            margin-left: rem(5);
        }
    }
}