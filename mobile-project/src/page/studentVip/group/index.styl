body {
    background: #f8f7ed;
    font-size: rem(10);
    margin: 0;
}
.student-vip-group {
	.main {
		width: 80%;
		margin: 0 auto;
		.underline {
			border-bottom: 1px solid #acacac;
		    display: inline-block;
		    width: 100%;
		    text-align: center;
			span {
				background-color: #f8f7ed;
				font-size:rem(16);
				color:#3d3d3e;
				padding: rem(5);
			}
		}
		.button {
			border-radius:rem(100);
			width:100%;
			height:rem(50);
			font-size:rem(20);
			color:#fefefe;
			line-height:rem(50);
			text-align: center;
			margin: rem(20) auto;
		}
		.qq {
			margin-top: rem(20);
			.button {
				background:#954ba8;
			}
		}
		.discuss {
			margin-top: rem(50);
			.button {
				background:#37bc61;
			}
		}
		.tips {
			font-size: rem(15);
			color: #9d9d9e;
			line-height: rem(22);
			margin-top: rem(16);
			word-wrap: break-word;
    		word-break: break-all;
		}
	}
}