.org-dynamic-style {
    background-color: #FFF;
    padding: rem(15) rem(10)
    margin-bottom: rem(10);
    position: relative;
    > p {
        width: 100%;
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
        line-height: rem(23);
        font-size: rem(14);
        color: #3c3d3d;
        > span {
            font-size: rem(10);
            color: #ff6c00;
            border: 1px solid #ff6c00
            padding: rem(3);
            margin-right: rem(9);
        }
    }
    > .dynamic-detail {
        clearfix();
        margin: rem(15) 0;
        .l {
            float: left;
            width: rem(105);
            height: rem(60);
            img {
                width: 100%;
                height: 100%;
            }
            margin-right: rem(10);
        }
        .cell {
            display: table-cell;
            width: rem(4000);
            p {
                font-size: rem(12);
                color: #9d9e9e;
                max-height: rem(69);
                text-overflow: ellipsis;
                overflow: hidden;
                white-space: normal;
                word-wrap: break-word;
                word-break: break-all;
            }
        }
    }
    > .dynamic-extra {
        clearfix();
        .zan-num {
            float: left;
            font-size: rem(12);
            color: #9d9e9e;
        }
        .post-time {
            float: right;
            font-size: rem(11);
            color: #9d9e9e;
        }
    }

    > a {
        position: absolute;
        width: 100%;
        height: 100%;
        top: 0;
        left: 0;

    }
}