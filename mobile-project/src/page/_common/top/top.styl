.topTitle {
    display: none;
    background: white;
    position: fixed;
    min-height: rem(50);
    transition: height 0.3s;
    top: 0;
    left: 0;
    right: 0;
    z-index: 200;
    border-bottom: 1px solid #ebeced;

    .nav-button-back, .gxs-icon {
        height: rem(45);
        min-width: rem(40);
        display: none;
    }
    .gxs-icon {
        img {
            margin-top: rem(10);
            height: rem(30);
            padding: 0 rem(10);
        }
    }
    .nav-button-back {
        img {
            width: rem(20);
            padding-top: rem(13);
            padding-left: rem(10);
            padding-bottom: rem(10);
            padding-right: rem(10);
        }
    }
    .downloadApp {
        //display: none;
        display: inline-block;
        background: #FF9100;
        color: white;
        font-family: PingFang-SC-Regular;
        font-size: rem(12);
        color: #ffffff;
        letter-spacing: rem(0);
        line-height: rem(12);
        text-align: center;
        padding: rem(8) rem(10);
        border-radius: rem(2);
        position: absolute;
        right: rem(50);
        top: rem(10);
    }
    .menu-nav-button {
        //display: none;
        display: inline-block;
        position: absolute;
        right: 0;
        width: rem(45);
        top: 50%;
        margin-top: rem(-20);
        text-align: center;
        font-size: rem(20)
        .icon {
            display: block;
        }
        .text {
            font-size: rem(12);
        }
    }
}

.download-app-mask {
    display: none;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: black;
    z-index: 800;
    opacity: 0.8;
    .content {
        width: 100%;
        text-align: right;
        img {
            width: 60%;
            margin-top: rem(30);
        }
    }
}