@import "page/_base/base.styl";
@import "page/_common/nav_bar/nav_bar.styl";
body {
    background-color: #fff;
}

.content-main {
    font-size: rem(12);
    line-height: 1.8;
    padding: rem(10);
    color: #404040;
    padding-bottom: rem(50);
}
table{
     border: 1px solid #999;
        border-width: 0 0 1px 1px;
    width :100%;
}
.table-scroll{
    
    td{
        border: 1px solid #999;
        border-width: 1px 1px 0 0;
        text-align: center;
    }
}
img {
    width: 100%;
}

.title {
    text-align: center;
    margin: rem(10);
    font-size: rem(14);
}

.f-w {
    font-weight: bold;
}

.button {
    display: inline-block;
    margin: 0;
    text-align: center;
    height: rem(40);
    padding: 0 rem(10);
    color: #fff;
    border: 0;
    background-color: #06c1ae;
    font-size: rem(16);
    vertical-align: middle;
    line-height: rem(40);
    box-sizing: border-box;
    cursor: pointer;
    -webkit-user-select: none;
    width: 100%;
    position: fixed;
    bottom: 0;
}

.grey {
    background-color: #ccc;
}

