.five-tab {
    background: white;
    width: 100%;
    margin-bottom: rem(10);
    .item-first-nav {
        width: 32%;
        display: inline-table;
        //border-bottom: 1px solid #f4f4f4;
        border-right: 1px solid #f4f4f4;
        text-align: center;
        height: rem(150);
        padding-top: rem(10);
        img {
            width: 50%;
        }
    }
    .item-second-nav {
        width: 48%;
        display: inline-block;
        border-right: 1px solid #f4f4f4;
        border-top: 1px solid #f4f4f4;
        padding: rem(20) 0;
        padding-left: rem(10);
        position: relative;
        .second-nav-avart {
            position: absolute;
            right: rem(10);
            top: rem(15);
            width: 35%;
        }
    }
    .title {
        font-family: PingFang-SC-Medium;
        font-size:rem(15);
        color:#3c3d3d;
        line-height:rem(16);
        width: 100%;
        text-align: center;
        margin-top: rem(6);
    }

    .title2 {
        font-family: PingFang-SC-Medium;
        font-size:rem(15);
        color:#3c3d3d;
        line-height:rem(16);
        width: 57%;
        text-align: center;
        margin-top: rem(6);
    }
    .sub {
        font-family: PingFang-SC-Regular;
        font-size: rem(12);
        color: #9d9e9e;
        letter-spacing: rem(0);
        line-height: rem(12);
        margin-top: rem(5);
        margin-bottom: rem(15);
        &.sub-other {
            width: 60%;
        }
    }
}