.initAds {
    position: fixed;
    top: 34%;
    left: rem(10);
    bottom: 0;
    right: rem(10);
    z-index: 153;
    .close-box {
        position: absolute;
        right: rem(10);
        top: rem(-21);
        width: rem(30);
        height: rem(30);
        img {
            width: rem(30);
            height: rem(30);
        }
    }
    .connect-box {
        position: absolute;
        top: rem(6);
        right: rem(22);
        width: rem(5);
        height: rem(18);
        background: #d8d8d8;
    }
    .no-more {
        position: absolute;
        left: rem(10);
        top: rem(-14);
        padding: rem(10);
        .no-more-box {
            height: rem(16);
            width: rem(16);
            background: #fff;
            border-radius: 50%;
        }
    }
    .no-more-text {
        position: absolute;
        left: rem(45);
        top: rem(-2);
        font-family: PingFang-SC-Regular;
        font-size: rem(14);
        color: #DDDDDE;
        line-height: rem(14);
    }
    .ad-box {
        width: 100%;
        position: absolute;
        top: rem(23);
        img {
            width: 96%;
            margin-left: 2%;
            border-radius: rem(4);
        }
    }
}
.init-mask {
    background: black;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    position: fixed;
    opacity: 0.8;
    z-index: 152;
}