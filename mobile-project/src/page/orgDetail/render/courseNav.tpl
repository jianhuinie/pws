<div class="course-nav course-nav-fixed hide">

    <div class="tab">
        <div class="item{{if status == 'all'}} active{{/if}}" data-order="all" data-index="0">综合</div>
        <div class="item{{if status == 'time'}} active{{/if}}" data-order="time" data-index="1">最新</div>
        <div class="item{{if status == 'popular'}} active{{/if}}" data-order="popular" data-index="2">人气</div>
        <div class="item{{if status == 'price'}} active{{/if}}" data-order="price" data-index="3">价格</div>
        <div class="all" data-status = "0" data-index="4">
            <span>全部类型</span>
            <img src="https://imgs.genshuixue.com/0cms/d/file/content/2016/11/5822afb39cc86.png">
        </div>
    </div>

    <div class="box">
        <div class="it{{if index == '5'}} active{{/if}}" data-type="5" style="border-top: 1px solid #f7f7f8" data-value="全部类型">
            <span>全部类型</span>
            <img src="https://imgs.genshuixue.com/0cms/d/file/content/2016/11/582a7b2594dae.png" {{if index!='5'}}class="hide"{{/if}}>
        </div>
        <div class="it{{if index == '3'}} active{{/if}}" data-type="3" data-value="直播课">
            <span>直播课</span>
            <img src="https://imgs.genshuixue.com/0cms/d/file/content/2016/11/582a7b2594dae.png" {{if index!='3'}}class="hide"{{/if}}>
        </div>
        <div class="it{{if index == '2'}} active{{/if}}" data-type="2" data-value="线下班课">
            <span>线下班课</span>
            <img src="https://imgs.genshuixue.com/0cms/d/file/content/2016/11/582a7b2594dae.png" {{if index!='2'}}class="hide"{{/if}}>
        </div>
        <div class="it{{if index == '1'}} active{{/if}}" data-type="1" data-value="1对1">
            <span>1对1</span>
            <img src="https://imgs.genshuixue.com/0cms/d/file/content/2016/11/582a7b2594dae.png" {{if index!='1'}}class="hide"{{/if}}>
        </div>
        <div class="it{{if index == '4'}} active{{/if}}" data-type="4" data-value="视频课">
            <span>视频课</span>
            <img src="https://imgs.genshuixue.com/0cms/d/file/content/2016/11/582a7b2594dae.png" {{if index!='4'}}class="hide"{{/if}}>
        </div>
    </div>
</div>
