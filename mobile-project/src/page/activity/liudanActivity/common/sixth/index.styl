.article5 {
        background: #fff;
        padding: rem(50) rem(20) rem(70) rem(20);
        .little-title {
            font-size: rem(18);
            color: #2D2D2E;
            letter-spacing: 0;
            margin-bottom: rem(10);
        }
        .vip-title {
            position: relative;
            top: rem(-18);
        }
        .title {
            padding: 0 rem(30);
            margin-bottom: rem(30);
            font-size: rem(27);
            color: #2D2D2E;
            letter-spacing: 0;
            line-height: rem(31);
            span {
                color: #F61B11;
            }
        }
        #form {
            .input {
                border: none;
                outline: none;
                width: 100%;
                height: rem(45);
                background: #F8F8F8;
                margin-bottom: rem(10);
                text-align: center;
                font-size: rem(16);
                color: #3D3D3E;
                line-height: rem(45);
                input {
                    background: #F8F8F8;
                }
            }
            .submit {
                width: 100%;
                height: rem(50);
                margin-top: rem(10);
                background: #F61B11;
                color: #fff;
                font-size: rem(20);
                line-height: rem(50);
            }
        }
        .protocol {
            padding: 0 rem(58);
            margin-top: rem(20);
            font-size: rem(12);
            color: #9D9D9E;
            line-height: rem(17);
        }
        .to-home {
            margin: rem(30) auto rem(50) auto;
            width: rem(140);
            img {
                width: rem(18);
                height: rem(18);
            }
            span {
                margin-left: rem(5);
                font-size: rem(16);
                color: #F89C1C;
                line-height: rem(16);
                vertical-align: bottom;
            }
        }
    }