.article1 {
        background: #fff;
        padding: rem(50) rem(15) rem(70) rem(15);
        .title, .little-title {
            text-align: center;
        }
        .title {
            font-size: rem(26);
            color: #2D2D2E;
            line-height: rem(24);
        }
        .little-title {
            margin: rem(14) auto rem(24) auto;
            font-size: rem(15);
            color: #3D3D3E;
            letter-spacing: rem(-0.31);
            line-height: rem(15);
        }
        .vip-img {
            text-align: center;
            margin-top: rem(40);
        }
        .content {
            margin-bottom: rem(10);
            font-size: rem(15);
            color: #9D9D9E;
            letter-spacing: rem(-0.31);
            line-height: rem(20);
            margin-top: rem(25);
        }
        .circle-goods {
            .circle-little {
                background-image: url(/src/page/activity/liudanActivity/common/img/header_jinpai.png);
                background-repeat: no-repeat;
                background-size: rem(112) auto;
                background-position: center;
            }
            .good1 {
                top: rem(-30);
                left: 50%;
                margin-left: rem(-40);
            }
            .good2 {
                top: rem(50);
                left: rem(-30);
            }
            .good3 {
                top: rem(50);
                right: rem(-30);
            }
            .good4 {
                bottom: 0;
                left: 0;
            }
            .good5 {
                bottom: 0;
                right: 0;
            }
        }
    }