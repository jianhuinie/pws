.bottom {
  background: #ffffff;
  overflow: hidden;
  font-size: rem(13);
  padding: rem(5);
  position: fixed;
  width: 100%;
  height: rem(49);
  bottom: rem(0);
  z-index: 2;
  display: flex;
  justify-content: space-around;
  border-top: 1px solid #ebeced;

  > div {
    position: relative;
    height: rem(40);
    line-height: rem(40);
    color: #6d6d6d;
    text-align: center;
    overflow: hidden;
    font-size: rem(13);
    vertical-align: middle;
    margin-right: rem(4);
    flex: 1;
    &:last-child {
      padding-left: 0;
      margin-right: 0;
    }
    span {
      vertical-align: super;
      color: #6d6d6e;
      font-size: rem(14);
      &.orange {
        color: #ff9100;
      }
    }

  }
  .bottom-favor {
    display: none;
  }
  .bottom-favor {
    .favor {
      img {
        width: rem(20);
        height: rem(20);
        display: inline-block;
      }
      .orange {
        color: #ff9100;
      }
      .coffee {
        color: #795548;
      }
      .red {
        color: #f95e5e;
      }

    }
  }
  .bottom-favor-class-super {
    span {
      display: inline-block;
      color: #fff;
      font-size: rem(14);
      margin-top: 0;
    }
  }
  .bottom-item-advisory,.bottom-item-liudan,.bottom-item-signup {
    min-width: rem(100);
  }

  .icon-share {
    font-size: rem(20);
    vertical-align: baseline;
    margin-right: rem(3);
    color: #6d6d6e;
  }
  .consult-icon {
    width: rem(20);
    height: rem(20);
    margin: 0 auto;
    margin-top: rem(-1);
    display: inline-block;
    margin-right: rem(2);
    img {
      vertical-align: inherit;
    }
  }
  .small-width {
    .consult {
      position: relative;
      padding-right: 0;
      span {
        display: block;
        font-size: rem(12);
        margin-top: rem(-9);

      }
    }

    .bottom-share {
      &.bottom-share-class-vip {
        width: rem(40);
        margin:0 auto;
        line-height: rem(16);
      }
      &.bottom-share-normal {
        width: rem(40);
        margin:0 auto;
      }
      .icon-share {
        font-size: rem(19);
        vertical-align: text-top;
        margin-right: rem(5);
        display: block;
        margin-top: rem(3);
        color: #6d6d6e;
      }
      span {
        display: block;
        font-size: rem(12);
        margin-top: rem(-12);

      }
    }
    .favor {
      line-height: rem(14);
      img {
        width: rem(20);
        height: rem(20);
        vertical-align: text-top;
        display: block;
        color: #6d6d6e;
        margin: 0 auto;
        margin-top: rem(1)
      }
      span {
        display: block;
        font-size: rem(12);
        margin-top: rem(2);
      }
    }
    .favor-icon, .consult-icon {
      display: block;
      margin-right: auto;
    }
  }
  .item-class-super {
    background: #00bcd4;
    .enrolling {
      background: #ff5252;

    }
    .btn-appoint,.btn-appoint-liudan {
      background-color: #212121;
    }
    .live {
      background: #ff5252;
    }
    .click-false {
      background: #727272;
    }
    .consult,.bottom-share {
      background-color: #00bcd4;
      span {
        display: inline-block;
        color: #ffffff;
        font-size: rem(14);
        margin-top: 0;
      }
    }
    .finish,.has_pay {
      background: #00bcd4;
    }
    .app-finish {
      background: #9d9e9e;
    }
  }
  .item-class-vip {
    .enrolling-orange {
      background: #ff9100;

    }
    .padding-width {
      padding-right: rem(20);
    }
    .enrolling-coffee {
      background: #a4877d;

    }
    .btn-appoint-orange,.btn-appoint-liudan-orange {
      background-color: #ff6c00;
      img {
        padding-right: rem(5);
        vertical-align: text-bottom;
      }
    }
    .btn-appoint-coffee,.btn-appoint-liudan-coffee {
      background-color: #795548;
      img {
        padding-right: rem(5);
        vertical-align: text-bottom;
      }
    }
    .btn-appoint-red,.btn-appoint-liudan-red {
      background-color: #f95e5e;
      img {
        padding-right: rem(5);
        vertical-align: text-bottom;
      }
    }

    .live-orange,.live-coffee {
      background: #48b14b;
    }
    .click-false-orange,.click-false-coffee {
      background: #727272;
    }
    .finish-orange, .finish-coffee,.finish-red {
      background: #3fa5f2;
    }
    .has_pay-orange,.has_pay-coffee {
      background: #3fa5f2;
    }
    .app-finish {
      background: #9d9e9e;
    }
  }

  .sign-up, .btn-appoint,.btn-appoint-liudan {
    width: 100%;
    height: rem(40);
    color: #ffffff;
    background: #9d9e9e;
    border-radius: rem(4);
    text-align: center;
    margin: 0 auto;
    font-size: rem(16);
    border: none;
    position: absolute;
    left: 0;
    top:0;
  }
  .btn-appoint-red {
    background: #f95e5e;
  }
  .enrolling-red {
    background: #ff9100;
  }
}

.bottom-class-super {
  background: #00bcd4;
  border: none;
}
.display-none {
  display: none;
}

.share-mask {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0 ,0.7);
  z-index: 10;
  .content {
    position: absolute;
    top: rem(10);
    right: rem(33.008);
    width: rem(205.008);
    height: rem(142);
  }
}
.back-top-component {
  z-index: 9 !important;
}

.purchase {
    position: fixed;
    bottom: 0;
    z-index: 1000;
    width: 100%;
    height: rem(293);
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
    background-color: #fff;
    -webkit-transition: -webkit-transform 1s;
    transition: transform 1s;

    .close {
        float: right;
        margin: rem(12) rem(12) 0;
        width: rem(23);
        height: rem(23);
    }

    .name {
        clear: both;
        margin: 0 rem(10);
        padding-bottom: rem(14);
        font-size: rem(15);
        color: #3d3d3e;
        font-weight: bold;
        text-indent: rem(10);
        border-bottom: rem(1) solid #f3f4f5;
    }

    .way {
        margin: 0 rem(10);
        padding-top: rem(15);
        height: rem(83);
        border-bottom: rem(1) solid #f3f4f5;

        .title {
            font-size: rem(14);
            text-indent: rem(10);
            color: #3d3d3e;
        }

        .choose {
            margin: rem(10);
            width: rem(70);
            height: rem(28);
            border: rem(1) solid #ff9100;
            border-radius: rem(2);
            line-height: rem(28);
            font-size: rem(13);
            text-align: center;
            color: #ff9100;
        }
    }

    .quantity {
        margin: rem(15) rem(20);
        overflow: hidden;
        line-height: rem(22);

        .title {
            float: left;
            font-size: rem(14);
            color: #3d3d3e;
        }

        .operation, .num {
            float: right;
            margin-left: rem(1);
            width: rem(22);
            height: rem(22);
            text-align: center;
            line-height: rem(22);
            font-size: rem(13);
            background-color: #ededee;
        }
    }

    .sum {
        margin: rem(20) rem(20) 0;
        text-align: right;
        font-size: rem(13);
        color: #9d9d9e;
        line-height: rem(18);

        span {
            color: #ff9100;
            font-size: rem(16);
        }
    }

    .submit {
        margin-top: rem(15);
        height: rem(44);
        line-height: rem(44);
        background-color: #ff9100;
        text-align: center;
        color: #fff;
        font-size: rem(17);
    }
}


.class-vip-bottom-consultation {
    min-width: rem(40);
    max-width: rem(78);
    margin:0 auto;
    line-height: rem(16);
}
.normal-consultation {
    min-width: rem(40);
    max-width: rem(78);
    margin:0 auto;
    line-height: 0;
}