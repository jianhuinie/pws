.vip-wrapper,.jhx-price {
    background-color: #e63f35;
    overflow: hidden;
    position: relative;
    height: rem(54);
    .left-part {
        float: left;
    }
    .right-part {
        position: absolute;
        right: 0;
        height: rem(54);

        background-color: #c42f25;
    }
    .price {
        font-size: rem(30);
        display: inline-block;
        line-height: rem(55);
        margin-right: rem(3);
        color: #fff;
        padding-left: rem(5);
        padding-right: 0;

    }
    .text {
        display: inline-block;
        font-size: rem(10);
        color: #ffd6d6;
    }
    .price-text {
        line-height: 1.3;
    }
    .num {
        color: #fff;
        padding-left: rem(3);
    }
    .symbol {
        font-size: rem(21);
        vertical-align: middle;
    }
    .tip {
        display: inline-block;
        font-size: rem(12);
        padding-left: rem(10);
    }
    .line1 {
        color: #fff;
        padding-top: rem(6);
        font-size: rem(16);
    }
    .line2 {
        color: #ffd6d6;
    }
    .next-icon {
        display: inline-block;
        color: #fff;
        position: relative;
        top: rem(-10);
        right: rem(5);
        font-size: rem(15);
    }

}
.vip-wrapper {
    background-color: #934CA9;
    .left-part {
        width: 58%;
        .price {
            width: 100%;
            text-align: left;
            line-height: rem(10);
            margin-top: rem(6);
        }
        .vip-price {
            font-size: rem(14);
        }
        .ori-price {
            font-size: rem(12);
            margin-left: rem(5);
            color: #C9A6D2;
        }
        .enroll-num {
            color: #C9A6D2;
            margin-left: rem(7);
            font-size: rem(12);
        }
    }
    .right-part {
        background-color: #833A96;
        width: rem(125);
        a {
            background-color: #833A96;
            .vip-club {
                font-size: rem(14);
                margin-top: rem(7);
                text-align: center;
                color: #fff;
            }
            .cheap {
                font-size: rem(10);
                color: #C9A6D2;
                text-align: center;
            }
        }
        .next-icon {
            display: inline-block;
            color: #fff;
            position: relative;
            top: rem(-31);
            font-size: rem(10);
            margin-left: rem(98);
        }
        .next-icon {
            margin-left: rem(112);
        }
    }
}
