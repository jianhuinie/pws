}

.label {
    position: absolute;
    bottom: rem(20);
    right: rem(10);
    height: rem(25);
    line-height: rem(25);
    font-size: rem(15);
    text-align: center;
    padding: 0 rem(12);
    background-color: #ff9100;
    color: #fff;
}

.jhx {
    position: absolute;
    top: 0;
    right: 0;
    width: rem(59);
    height: rem(59);
}

//课程简介
.course-sum {
    background-color: #fff;
    width: 100%;
    padding-bottom: rem(13);
    position: relative;
    .jhx-price {
        background-color: #e63f35;
        overflow: hidden;
        position: relative;
        height: rem(54);
        .left-part {
            float: left;
        }
        .right-part {
            position: absolute;
            right: 0;
            height: rem(54);

            background-color: #c42f25;
        }
        .price {
            font-size: rem(35);
            color: #fff;
            display: inline-block;
            padding-right: 0;
        }
        .text {
            display: inline-block;
            font-size: rem(10);
            color: #ffd6d6;
        }
        .price-text {
            line-height: 1.3;
        }
        .num {
            color: #fff;
            padding-left: rem(3);
        }
        .symbol {
            font-size: rem(21);
            vertical-align: middle;
            margin-left: rem(5);
            margin-right: rem(-10);
        }
        .tip {
            display: inline-block;
            font-size: rem(12);
            padding-left: rem(10);
        }
        .line1 {
            color: #fff;
            padding-top: rem(8);
            font-size: rem(16);
        }
        .line2 {
            color: #ffd6d6;
            margin-top: rem(-5);
        }
        .next-icon {
