        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
        margin: 0 rem(15) rem(7);
        padding-top: rem(17);
        overflow: hidden;
    }
    .price {
        padding-left: rem(15);
        margin-bottom: rem(7);
        line-height: 1;
        .current {
            font-size: rem(28);
            color: #ff6c00;
            margin-right: rem(5);
        }
        .original {
            font-size: rem(14);
            color: #9d9e9e;
            text-decoration: line-through;
            margin-right: rem(5);
        }
        .unit {
            font-size: rem(15);
        }
    }
    .countdown {
        padding-left: rem(15);
        margin-bottom: rem(10);
    }
    .student {
        font-size: rem(14);
        color: #9d9e9e;
        width: 100%;
        padding-left: rem(15);
        margin-bottom: rem(8);
    }
}

.sub-icons {
