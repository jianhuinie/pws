//顶部轮播图
.head {
    position: relative;

}

.top-sliders {

    width: 100%;
    height: rem(180);
    overflow: hidden;
    ul {
        position: relative;
        height: 100%;
        li {
            position relative;
            width: 100%;
            height: rem(180);
            overflow: hidden;
            float: left;
        }
    }
    .slide-control {
        position: absolute;
        bottom: 0;
        right: rem(5);
        .slide-position {
            width: rem(8);
            height: rem(8);
            border: 1px solid #fff;
            border-radius: 50%;
