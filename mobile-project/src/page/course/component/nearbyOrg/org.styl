.nearby-org {
    background: white;
    padding: rem(10);
    margin-bottom: rem(10);
    .start-title{
        text-align : center;
        .title{
            display : inline;
            font-size:rem(17);
            color:#3c3d3d;
            margin:rem(0) rem(3);
        }

        .icon-star_all{
            color:#ebeced;
            font-size:rem(10);
        }
        .block-line{
            position : relative;
            top:rem(-2);
            .line{
                top:rem(5);
                left: rem(-32);
                position : absolute;
                display : block;
                width:rem(30);
                height:rem(1);
                font-size : rem(0);
                border-top:1px solid #ebeced;
            }
            .line-sub{
                width:rem(10);
                left: rem(-10);
                top:rem(7);
            }
        }
        .block-line-right{
            .line{
                left:rem(14);
            }
            .line-sub{
                left: rem(12);
            }
        }
        .start-title-sub{
            margin-top:rem(2);
            font-size:rem(12);
            color:#9d9e9e;
            text-align:center;
        }
    }
    .org-list {
        //padding: 0 rem(10);
        .org-item {
            position: relative;
            padding: rem(10) 0 rem(10) rem(110);
            overflow: hidden;
            min-height: rem(110);
            margin: rem(10) 0;
            border-bottom: 1px solid #f4f4f4;
            img {
                height: rem(70);
                width: rem(80);
                position: absolute;
                left: rem(10);
                top: rem(0);
                border-radius: rem(5);
            }
            .org-info {
                .org-name {
                    font-family:PingFang-SC-Regular;
                    font-size:rem(15);
                    color:#3c3d3d;
                    letter-spacing:rem(-0.38);
                    line-height:rem(15);
                    text-align:left;
                }
                span {
                    font-family: PingFang-SC-Regular;
                    font-size: rem(12);
                    color: #9d9e9e;
                    letter-spacing: rem(-0.38);
                    line-height: rem(15);
                    height: rem(15);
                    text-align: left;
                }
                .third-nav {
                    position: relative;
                    .location {
                        position: absolute;
                        right: rem(10);
                    }
                }
            }
        }
    }
    .has-more {
        font-family: PingFang-SC-Regular;
        font-size: rem(14);
        color: #ff9100;
        letter-spacing: rem(-0.81);
        text-align: center;
        line-height: rem(30);
        height: rem(30);
        //margin-top: rem(10);
        &.hide {
            display: none;
        }
    }
}