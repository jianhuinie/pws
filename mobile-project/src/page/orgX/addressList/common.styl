@import "page/_base/base.styl";
@import "page/_common/nav_bar/nav_bar.styl";
// @import "page/college/ui/search/index.styl";

$white = #fff;
clearfix() {
    &:before,
    &:after {
        content: " ";
        display: table;
    }
    &:after {
        clear: both;
    }
    zoom: 1;
}
.clearfix {
    clear: both;
}

.hidden {
    display: none !important;
}

.primary-content {
    color: #ff9100;
}
