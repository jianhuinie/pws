.topTitle {
    //display: none;
    opacity: 0;
    background: white;
    position: fixed;
    min-height: rem(50);
    //height: 0;
    transition: height 0.3s;
    top: 0;
    left: 0;
    right: 0;
    z-index: 10;
    border-bottom: 1px solid #ebeced;

    .gsx-icon {
        display: none;
        img {
            width: 20%;
            padding: rem(10) rem(20);
        }
    }
    .nav-button-back {
        display: none;
        height: rem(45);
        min-width: rem(40);
        img {
            width: rem(20);
            padding: rem(15) rem(10) rem(10) rem(10);
        }
    }
    .downloadApp {
        //display: none;
        display: inline-block;
        background: #FF9100;
        color: white;
        font-family: PingFang-SC-Regular;
        font-size: rem(12);
        color: #ffffff;
        letter-spacing: rem(0);
        line-height: rem(12);
        text-align: center;
        padding: rem(8) rem(10);
        border-radius: rem(2);
        position: absolute;
        right: rem(50);
        top: rem(10);
    }
    .nav-button {
        //display: none;
        display: inline-block;
        position: absolute;
        right: 0;
        width: rem(45);
        top: 50%;
        margin-top: rem(-20);
        text-align: center;
        font-size: rem(20);
        .icon{
            display:block;
        }
        .text {
            font-size: rem(12);
            margin-top: rem(2);
        }
    }
}

.download-mask {
    display: none;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 1000;
    background: black;
    opacity: 0.8;
    .content {
        width: 100%;
        text-align: right;
        img {
            width: 60%;
            margin-top: rem(30);
        }
    }
}