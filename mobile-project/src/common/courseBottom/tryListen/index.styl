@import 'common/staySingle/staySingle.styl'
.tryListen {
    flex: 1.5;
    height: rem(50);
    line-height: rem(50);
    padding-bottom: rem(50);
    font-size: rem(15);
    font-family: PingFang-SC-Regular;
    letter-spacing: rem(0.53);
    &.orange {
        background: #FF9100;
        color: white;
    }
    &.red {
        background: #ff7575;
        color: white;
    }
    &.coffee {
        background: #8C6E63;
        color: white;
    }
}
