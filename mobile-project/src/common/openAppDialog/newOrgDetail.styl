#main {
    .main-page {
        display: inline;
        position: relative;
        z-index: 0;
    }
    .id-code {
        background: #ffffff;
        margin-top: rem(10);
        font-size: rem(14);
        color: #3c3d3d;
        padding-left: rem(10);
        line-height: 1;
        margin-bottom: rem(94);

        .id, .code {
            padding-right: rem(10);
        }
        .id {
            padding-bottom: rem(15);
            padding-top: rem(10);
        }
        .code {
            padding-top: rem(10);
            padding-bottom: rem(15);
            border-top: 1px solid #f2f4f5;

            .picture {
                float: right;
                width: rem(20);
                height: rem(20);
                margin: 0 auto;
                text-align: center;
                line-height: rem(20);

                i {
                    font-size: rem(20);
                }
            }
        }
        span {
            display: inline-block;
            font-size: rem(14);
        }
        .id-detail {
            float: right;
            color: #9d9d9e;
        }
    }
    .qrcode-container {
        display: none;
        position: fixed;
        top: 0;
        left: 0;
        height: 100%;
        width: 100%;
        z-index: 5;

        .mask {
            position: absolute;
            height: 100%;
            width: 100%;
            background-color: #000;
            opacity: 0.5;
        }
        .front {
            position: absolute;
            left: 50%;
            top: 50%;
            padding: rem(30);
            transform: translate3d(-50%, -50%, 0);
            background-color: #FFF;
            border-radius: rem(3);
        }
        .label-group {
            font-size: rem(13);
            margin: 0 auto;
            text-align: center;

            .label-name {
                float: left;
            }
            .org-name {
                margin-left: rem(60);
                text-align: left;
            }
        }
        #qrcode {
            margin: 0 auto;
            text-align: center;
            width: rem(180);
            height: rem(180);
            padding: rem(10);

            canvas {
                width: 100%;
            }
        }
        .hint {
            text-align: center;
            color: #9D9D9D;
            font-size: rem(12);
        }
        .cancel {
            font-size: rem(22);
            font-weight: bold;
            position: absolute;
            right: rem(9);
            top: rem(9);
            color: #9D9D9D;
        }
    }
}

#main {
    .bottom {
        position: fixed;
        bottom: 0;
        width: 100%;
        height: rem(50);
        font-size: rem(14);
        line-height: rem(50);
        background-color: #ffffff;
        box-shadow: 0 0 rem(4) #cccccc;
        z-index: 10;
        display: flex;
        justify-content: space-between;

        .org-focus {
            .icon {
                display: inline-block;
                vertical-align: middle;
                margin-top: rem(-2);
                background-image: url('https://imgs.genshuixue.com/0cms/d/file/content/2015/10/5634293eefde0.png');
                height: rem(17);
                width: rem(17);
                background-size: auto rem(17);
                background-repeat: no-repeat;
                margin-right: rem(2);
            }
            span {
                font-size: rem(14);
            }
        }
        .no_focus {
            .icon {
                background-position-x: rem(-17);
            }
            color: #FD9126;
        }
        .single_focus {
            .icon {
                background-position-x: -0;
            }
        }
        .double_focus {
            .icon {
                background-position-x: rem(-37);
            }
        }
        .item {
            position: relative;
            flex: 1;
            display: inline-block;
            white-space: nowrap;
            text-align: center;

            &.selected {
                background-color: #eaebed;
            }
            .handle {
                margin: 0;
                text-align: center;
            }
            .icon {
                font-size: rem(10);
                //color: #dcddde;
            }
            .im-button, .btn-appoint, .btn-stay-single {
                /*background-color: #ff6c00;*/
                border-radius: rem(2);
                height: rem(36);
                width: rem(72)
                line-height: rem(36);
                margin: rem(7) auto 0 auto;
                /*color: #ffffff;*/

                //font-size: rem(16);
                display: inline-block;
                vertical-align: middle;
                margin-top: rem(-2);

            }
            .menu-container {
                position: absolute;
                top: rem(-10);
                left: 50%;
                width: 0;
                height: 0;
            }
            .menu-control {
                position: relative;
                width: rem(96);
                transform: translate(-50%, -100%);
                background-color: #ffffff;
                border: 1px solid #dcddde;

                .decoration-corner {
                    position: absolute;
                    bottom: rem(-10);
                    left: rem(43);
                    width: rem(15);
                    height: rem(10);

                    .corner {
                        position: relative;
                        width: rem(15);
                        height: rem(10);
                        overflow: hidden;

                        .corner-handle {
                            position: relative;
                            width: rem(11);
                            height: rem(11);
                            top: rem(-5);
                            border: 1px solid #dcddde;
                            background-color: #ffffff;
                            transform: rotate(45deg);
                            transform-origin: center;
                            margin: 0 auto;
                        }
                    }
                }
                li {
                    text-align: center;
                    color: #3d3d3d;
                    height: rem(45);
                    line-height: rem(45);
                    border-top: 1px solid #dcddde;

                    &:first-child {
                        border-top: 0 none
                    }
                }

            }

            .btn-stay-single, .btn-appoint {
                background-color: #3fa5f2;
                color: #fff;
                img {
                    padding: 0 rem(1);
                }
            }

        }
    }
    .back-top {
        display: none;
        width: rem(40);
        height: rem(40);
        position: fixed;
        z-index: 999;
        bottom: rem(58);
        right: rem(10);
        line-height: rem(37);
        color: #ffffff;

        img {
            width: 100%
        }
    }
}

.fsdlg-container {
    bottom: rem(50);
}

.course-page, .teacher-page, .active-page {
    margin-top: rem(10);
}

.more-course-ajax, .more-teacher-ajax, .more-dynamic-ajax {
    text-align: center;
    color: #9d9e9e;
    font-size: rem(13);
    height: rem(40);
    border-top: 1px solid #e6e6e6;
    line-height: rem(40);
    background-color: #fff;

    > div:first-child {
        text-align: left;
        margin: 0 auto;
        position: relative;
        width: rem(93);

        > i {
            right: 0;
            top: rem(14);
        }
    }
}

.more-teacher-ajax {
    border-top: none;
}

.more-button.loading .spinner {
    display: block;
}

.more-button.loading {
    > div:first-child {
        display: none;
    }
}

.more-button {
    .more-loading {
        display: none;
    }
}

.more-button .spinner {
    display: none;
    margin: 0 auto;
    width: rem(50);
    height: 100%;
    text-align: center;
    font-size: rem(10);
    padding: rem(5) 0;
    > div {
        background-color: #ff7f00;
        height: 100%;
        width: rem(6);
        display: inline-block;
        -webkit-animation: more-button-delay 1.2s infinite ease-in-out;
        animation: more-button-delay 1.2s infinite ease-in-out;
    }
    .rect2 {
        -webkit-animation-delay: -1.1s;
        animation-delay: -1.1s;
    }
    .rect3 {
        -webkit-animation-delay: -1s;
        animation-delay: -1s;
    }
    .rect4 {
        -webkit-animation-delay: -0.9s;
        animation-delay: -0.9s;
    }
    .rect5 {
        -webkit-animation-delay: -0.8s;
        animation-delay: -0.8s;
    }
}

.ajax-loading {
    height: rem(100);
    background: url(../img/auth/loading.gif) center no-repeat;
    font-size: rem(16);
    line-height: rem(100);
    text-align: center;
}

@-webkit-keyframes more-button-delay {
    0%, 40%, 100% {
        -webkit-transform: scaleY(0.4)
    }
    20% {
        -webkit-transform: scaleY(1.0)
    }
}

@keyframes more-button-delay {
    0%, 40%, 100% {
        transform: scaleY(0.4);
        -webkit-transform: scaleY(0.4);
    }
    20% {
        transform: scaleY(1.0);
        -webkit-transform: scaleY(1.0);
    }
}

