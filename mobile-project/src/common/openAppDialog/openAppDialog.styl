
@import './fullScreenDialog.styl';
@import './weixinDownloadLayer.styl';


.fsdlg-body {
    color: #000;
    >ul {
        padding: rem(10) rem(30) rem(20);
        >li {
            border: none;
        }

    }

    .btn {
        color: #fff;
        font-size: rem(12);
        padding: rem(12) 0;
        border-radius: rem(6);
    }

    .btn-primary {
        background: $brand-primary;
        margin-bottom: rem(10);
    }

    .btn-info {
        background: $brand-info;
    }
}