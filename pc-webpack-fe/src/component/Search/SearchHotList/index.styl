.search-list-div {
    margin-top: -15px;
    border: 1px solid #CDCDCE;
    .search-list-ul,.search-history-list {
        padding: 0 10px;
        margin-bottom: 0;
        background: #ffffff;
        .analysis-haoke-log {
            &:hover {
                color: #FF6C00;
            }
        }
        .history-list-item {
            &:hover {
                color: #FF6C00;
            }
        }
        li {
            list-style: none;
            border-bottom: 1px solid #f8f8f8;
            line-height 45px;
            font-family: PingFangSC-Regular;
            font-size: 14px;
            color: #9D9D9E;
            letter-spacing: 0;
            position: relative;
            cursor: pointer;
            span {
                color: #3D3D3E;
            }
            icon {
                position: absolute;
                right: 10px;
                top: 14px;
            }
            .icon-bin {
                top: 36px;
            }
        }
    }
}