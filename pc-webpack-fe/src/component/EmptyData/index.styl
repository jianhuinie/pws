.empty-data {
    padding: 80px 0;
    text-align: center;
    img {
        width: 94px;
        height: 100px;
    }
    .text {
        font-size: 16px;
        color: #9D9D9E;
        margin-top: 16px;
    }
    a {
        display: block;
        background: #FF6C00;
        border-radius: 5px; 
        width: 167px;
        height: 42px;
        line-height: 42px;
        color: #fff;
        font-size: 16px;
        margin: 0 auto;
        margin-top: 40px;
        &:hover {
            text-decoration: none;
        }
    }   
}