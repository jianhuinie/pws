.multi-section {
    .check-more {
        width: 124px;
        height: 34px;
        border: 0.5px solid #CDCDCD;
        line-height: 32px;
        text-align: center;
        position: relative;
        left: 50%;
        transform: translateX(-50%);
        margin-top: 30px;
        border-radius: 24px;
        span {
            font-size: 14px;
            color: #5D5D5D;
        }
    }
}