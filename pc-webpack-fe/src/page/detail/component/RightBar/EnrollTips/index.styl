.enroll-tips {
    background: #fff;
    padding: 30px;
    margin-bottom: 15px;
    .title {
        font-size: 16px;
        color: #3D3D3E;
        padding-bottom: 22px;
        border-bottom: 1px solid #EDEDEE;
    }
    .tip {
        font-size: 16px;
        color: #6D6D6E;
        cursor: pointer;
        margin-top: 10px;
        &:hover {
            background: #F3F3F4;
        }
        span {
            display: inline-block;
            width: 4px;
            height: 4px;
            border-radius: 50%; 
            background: #DDDDDE;
            margin-right: 16px;
            position: relative;
            bottom: 3px;
        }
    }
}