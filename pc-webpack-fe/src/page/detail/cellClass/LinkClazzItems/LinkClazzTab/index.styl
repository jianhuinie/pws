.link-clazz-tab {
    width:100%;
    background: #F8F8F8;
    .link-clazz-item {
        cursor: pointer;
        width:212px;
        height:60px;
        text-align:center;
        background: #F8F8F8;
        font-size: 16px;
        color: #6D6D6E;
        padding: 20px 0;
        display: inline-block;
        &:last-child {
            margin: 0;
        }
        &.active {
            background: #fff;
            color: #0C0C0C;
        }
    }
}