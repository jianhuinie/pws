@import "../components/bottomTab/index.wxss";
page .all-videos .videos-container {
    padding-bottom: 110rpx;
}
page .all-videos .videos-container .title {
    color: #2d2d2e;
    font-size: 32rpx;
    font-weight: bold;
    margin: 40rpx 0;
}
page .all-videos .videos-container .video-item {
    margin-bottom: 20rpx;
    position: relative;
}
page .all-videos .videos-container .video-item image {
    width: 100%;
    height: 420rpx;
}
page .all-videos .videos-container .video-item image.icon-play {
    position: absolute;
    width: 100rpx;
    height: 100rpx;
    top: 34%;
    left: 42%;
}
page .all-videos .videos-container .video-item .video-name {
    color: #3d3d3e;
    font-size: 28rpx;
    font-weight: bold;
    text-align: left;
    text-indent: 10rpx;
}