@import '../../../resource/css/default.styl';

.series-list {
    flex-grow: 1;
    display: flex;
    flex-direction: column;
    &-operation {
        height: 66px;
        padding: 19px 22px 19px 20px;
        display: flex;
        justify-content: space-between;
        &-add {
            &-button {
                font-size: 12px;
                line-height: 12px;
                padding: 8px 16px;
            }
        }
    }
}