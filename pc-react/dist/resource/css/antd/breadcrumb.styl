.child-page .ant-breadcrumb {
    color: $grey_500;
    font-size: 12px;
    line-height: 12px;
    background-color: $grey_30;
    border-bottom: 1px solid $grey_50;
    padding: 14px 0 14px 20px;
    height: 40px;
    a {
        color: $grey_500;
    }
    &-separator {
        margin: 0 5px;
        color: $grey_500;
    }
    > span:last-child {
        color: $grey_600;
    }
    a:hover {
        color: $pink;
    }
}