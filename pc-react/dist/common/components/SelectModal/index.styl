@import "../../../resource/css/default.styl";

.select-modal {
    .ant-modal-content {
        border-radius: 2px;
        .ant-modal-header {
            background-color: $grey_30;
            border-bottom: none;
            .ant-modal-title {
                font-size: 14px;
                color: $grey_600;
                line-height: 14px;
                font-weight: normal;
            }
        }
        .ant-modal-body {
            padding: 15px 15px 30px 15px;
        }
    }
}