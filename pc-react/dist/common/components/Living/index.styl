.living-wrapper {
    display: inline-flex;
    height: rem(10);
    width: rem(18);
    justify-content: space-between;
    align-items: flex-end;
    &-item {
        width: rem(2);
        background: #F95D5D;
        &.item1 {
            animation: mymove2 0.4s infinite 0.3s alternate;
        }
        &.item2 {
            animation: mymove 0.4s infinite 0.2s alternate;
        }
        &.item3 {
            animation: mymove 0.4s infinite 0.1s alternate;
        }
        &.item4 {
            animation: mymove 0.4s infinite 0.4s alternate;
        }
        &.item5 {
            animation: mymove2 0.4s infinite 0.5s alternate;
        }
    }
}

@keyframes mymove {
    from {
        height: rem(0);
    }
    to {
        height: rem(10);
    }
}
@keyframes mymove2 {
    from {
        height: rem(0);
    }
    to {
        height: rem(7);
    }
}