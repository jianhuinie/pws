    "pm2-prod-start": "ENV_TYPE=prod NODE_ENV=production pm2 start npm --name 'pc' -i 2 -- run start",
    "lint": "eslint components pages service util",
    "lint:fix": "eslint components pages service util --fix",
    "jest": "jest test --coverage --watch",
    "test": "npm run lint && npm run jest",
    "show:coverage": "open coverage/lcov-report/index.html",
    "dev": "ENV_TYPE=local NODE_ENV=development node server.js"
  },
