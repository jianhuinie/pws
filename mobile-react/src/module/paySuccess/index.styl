.pay-success-page {
    background-color: #fff;
    width: 100%;
    height: 100%;
    text-align: center;
    .success-icon {
        margin: auto;
        padding-top: rem(112);
        width: rem(50);
        height: rem(50);
        i {
            font-size: rem(50);
        }
    }
    .pay-success-text {
        margin-top: rem(15);
        font-size: rem(22);
        color: #3D3D3E;
    }
    .view-purchase-text {
        margin-top: rem(20);
        font-size: rem(14);
        color: #3D3D3E;
    }
    .enter-study-btn {
        margin: auto;
        margin-top: rem(42);
        width: rem(136);
        height: rem(40);
        background: #F95D5D;
        border-radius: rem(2);
        font-size: rem(16);
        color: #FFF;
        line-height: rem(40);
    }
}
.GSX_Dialog.GSX_Dialog_default {
    width: rem(270);
    margin-left: rem(12.5);
    img {
        width: rem(240);
        height: rem(240);
        display: block;
    }
}