@import "module/index.styl";
.purchase-record {
    background: #f2f3f4;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    &-list {
        margin: 0;
        padding 0;
        background: #fff;
        list-style: none;
        &-item {
            padding: 0 rem(15);
        }
    }
    .no-more {
        font-size: rem(14);
        color: #9D9D9E;
        letter-spacing: 0;
        line-height: rem(14);
        text-align: center;
        margin-top: rem(16);
        margin-bottom: rem(10);
    }
}