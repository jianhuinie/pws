@import "./common.styl";
.error-page {
    position: relative;
    img {
        width: 100%;
    }
    .error-text {
        width: 100%;
        height: rem(60);
        position: absolute;
        top: rem(120);
        left: 0;
        font-size: rem(20);
        color: #FFFFFF;
        letter-spacing: 0;
        div {
            text-align: center;
            height: rem(30);
            line-height: rem(30);
        }
    }
}