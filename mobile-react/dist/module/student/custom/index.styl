
@import "module/index.styl";
.contact-custom {
    height: 100%;
    text-align: center;
    background: #fff;
    overflow: hidden;
    &-qrcode {
        margin: rem(108) auto 0;
        width: rem(168);
        height: rem(168);
        padding: rem(6);
        box-shadow: 0 rem(1) rem(6) 0 #dfdfdf;
        border-radius: rem(8);
        img {
            width: 100%;
            height: 100%;
        }
    }
    &-title {
        margin-top: rem(32);
        font-size: rem(18);
        color: #3d3d3e;
        line-height: rem(18);
        font-weight: 700;
    }
    &-desc {
        margin-top: rem(16);
        font-size: rem(14);
        line-height: rem(14);
        color: #9d9d9e;
    }
    &-back {
        width: rem(263);
        margin-top: rem(32);
    }
    &-home {
        width: rem(263);
        margin-top: rem(15);
    }
}