@import "module/index.styl";
.avatar-container {
    position: relative;
    overflow: hidden;
    &-big {
        display: block;
        border-radius: 50%;
        border: rem(1) solid #ebebeb;
        margin: auto;
    }
    &-v {
        position: absolute;
        display: block;
        right: 0;
        bottom: 0;
        border-radius: 50%;
    }
}